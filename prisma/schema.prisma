generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int  @default(autoincrement()) @id
  email String @unique
  password String
  name String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
